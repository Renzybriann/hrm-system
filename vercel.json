{
    "version": 2,
    "builds": [
        {
            "src": "public/index.php",
            "use": "@juicyfx/vercel-php"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(css|js|images|fonts|icons)/(.*)",
            "dest": "public/$1/$2"
        },
        {
            "src": "/storage/(.*)",
            "dest": "public/storage/$1"
        },
        {
            "src": "/(.*)",
            "dest": "public/index.php"
        }
    ]
}